{
  "name": "main-fns",
  "version": "0.1.0",
  "description": "Javascript utility library created with TypeScript",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "author": "nabby27",
  "keywords": [
    "fns",
    "functions",
    "javascript",
    "js",
    "libraries",
    "libs",
    "ts",
    "typescript",
    "utilities",
    "utils"
  ],
  "license": "MIT",
  "private": false,
  "homepage": "https://nabby27.github.io/main-fns/",
  "repository": {
    "type": "git",
    "url": "https://github.com/nabby27/main-fns"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc --p tsconfig.server.json && tsc --p tsconfig.browser.json",
    "docs": "jsdoc -c jsdoc.json",
    "eslint:check": "npx eslint './src/**'",
    "eslint:fix": "npx eslint './src/**' --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:watch:coverage": "jest --watchAll --coverage",
    "test:coverage": "jest --coverage",
    "test:all": "npm run eslint:check && npm run test:coverage",
    "version:patch": "npm run test:all && npm run build && npm run docs && npm version patch",
    "version:minor": "npm run test:all && npm run build && npm run docs && npm version minor",
    "version:major": "npm run test:all && npm run build && npm run docs && npm version major"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">= 14.0.0"
  },
  "dependencies": {
    "date-fns": "^2.16.1",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.166",
    "@types/node": "^12.19.11",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^2.26.0",
    "better-docs": "^2.3.2",
    "codecov": "^3.8.1",
    "eslint": "^6.8.0",
    "eslint-plugin-tsdoc": "^0.2.10",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
